<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="3577" PreviewOptions.Zoom="1" PrintOptions.Printer="EPSON LQ-2180 ESC/P2" ReportOptions.CreateDate="40864.5028630556" ReportOptions.Description.Text="" ReportOptions.LastChange="40864.612411875" ScriptLanguage="PascalScript" ScriptText.Text="{parameter&#13;&#10;NIP&#13;&#10;TAHUN&#13;&#10;BULAN          ==&#62; tidak digunakan&#13;&#10;TglTTD&#13;&#10;}&#13;&#10;&#13;&#10;const strBulan = ['Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni', 'Juli', 'Agustus', 'September', 'Oktober', 'Nopember', 'Desember'];&#13;&#10;&#13;&#10;var BlnNm, AngkaTerbilang: String;&#13;&#10;       strWhere: String;&#13;&#10;&#13;&#10;function GetString(TXT: String; nLength: Integer=1; nOption: Integer=1; nStart: Integer=1): String;&#13;&#10;begin&#13;&#10;       Result:= '';&#13;&#10;       if (TXT = '') then exit;&#13;&#10;       if (nStart&#60;0) then exit;&#13;&#10;       if (nLength&#60;1) then exit;&#13;&#10;       case nOption of&#13;&#10;       1       :       // left string&#13;&#10;               Result:= Copy(TXT, 0, nLength);&#13;&#10;       2       :       // right string&#13;&#10;               Result:= Copy(TXT, Length(TXT) - nLength + 1, nLength);&#13;&#10;       3       :       // mid string&#13;&#10;               Result:= Copy(TXT, nStart, nLength);&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTanggal(TGL: TDateTime): String;&#13;&#10;begin&#13;&#10;  Result:= FormatFloat('00', DayOf(TGL)) + ' ' + strBulan[MonthOf(TGL)-1] + ' ' + IntToStr(YearOf(TGL));&#13;&#10;end;&#13;&#10;&#13;&#10;function Terbilang(Num: Double; UseDecimal: Boolean = False): String;&#13;&#10;var Res, NumStr, TmpStr: String;&#13;&#10;    Angka, Desimal: Double;&#13;&#10;const&#13;&#10;      Satu = 'Satu ';&#13;&#10;      Belas = 'Belas ';&#13;&#10;      Satuan = ['', 'Se', 'Dua ', 'Tiga ', 'Empat ', 'Lima ',&#13;&#10;                    'Enam ', 'Tujuh ', 'Delapan ', 'Sembilan '];&#13;&#10;      LenGroup = ['Puluh ', 'Ratus ', 'Ribu ', 'Juta ',&#13;&#10;                    'Milyar ', 'Trilyun '];&#13;&#10;begin&#13;&#10;  Desimal:= Abs(Frac(Num));&#13;&#10;  Angka:= Abs(Num) - Desimal;&#13;&#10;  if (Num&#60;0) then Res:= 'Minus '&#13;&#10;  else Res:= '';&#13;&#10;  NumStr:= FloatToStr(Angka);&#13;&#10;  case Length(NumStr) of&#13;&#10;    1  :&#13;&#10;      begin&#13;&#10;        if (NumStr = '1') then Res:= Res + Satu&#13;&#10;        else Res:= Res + Satuan[StrToInt(NumStr)];&#13;&#10;      end;&#13;&#10;    2 :&#13;&#10;      begin&#13;&#10;        case StrToInt(NumStr) of&#13;&#10;        11..19 : Res:= Res + Satuan[StrToInt(GetString(NumStr, 1, 2))] + Belas;&#13;&#10;        else&#13;&#10;          begin&#13;&#10;            Res:= Res + Satuan[StrToInt(GetString(NumStr, 1))];&#13;&#10;            if (GetString(NumStr, 1) = '1') then Res:= Res + 'puluh '&#13;&#10;            else Res:= Res + 'Puluh ';&#13;&#10;            if (GetString(NumStr,1, 2)&#60;&#62;'0') then&#13;&#10;              Res:= Res + Satuan[StrToInt(GetString(NumStr, 1, 2))];&#13;&#10;          end;&#13;&#10;        end&#13;&#10;      end;&#13;&#10;    3 :&#13;&#10;      begin&#13;&#10;        Res:= Res + Satuan[StrToInt(GetString(NumStr, 1))];&#13;&#10;        if (GetString(NumStr, 1)='1') then Res:= Res + 'ratus '&#13;&#10;        else Res:= Res + 'Ratus ';&#13;&#10;        Res:= Res + Terbilang(StrToFloat(GetString(NumStr, 2, 2)));&#13;&#10;      end;&#13;&#10;    4..6 :&#13;&#10;      begin&#13;&#10;        TmpStr:= GetString(NumStr, Length(NumStr) - 3);&#13;&#10;        if (TmpStr = '1') then Res:= Res + Satuan[StrToInt(TmpStr)] + 'ribu '&#13;&#10;        else&#13;&#10;          begin&#13;&#10;            Res:= Res + Terbilang(StrToFloat(TmpStr));&#13;&#10;            Res:= Res + 'Ribu ';&#13;&#10;          end;&#13;&#10;        Res:= Res + Terbilang(StrToFloat(GetString(NumStr, 3, 2)));&#13;&#10;      end;&#13;&#10;    else&#13;&#10;      begin&#13;&#10;        if ((Length(NumStr)&#60;=15)and(Length(NumStr)&#62;6)) then&#13;&#10;          begin&#13;&#10;            TmpStr:= '000000';&#13;&#10;            case Length(NumStr) of&#13;&#10;              7..9 :&#13;&#10;                begin&#13;&#10;                  TmpStr:= GetString(NumStr, Length(NumStr) - 6);&#13;&#10;                  if (TmpStr = '1') then Res:= Res + Satu&#13;&#10;                  else Res:= Res + Terbilang(StrToFloat(TmpStr));&#13;&#10;                  Res:= Res + 'Juta ';&#13;&#10;                end;&#13;&#10;              10..12 :&#13;&#10;                begin&#13;&#10;                  TmpStr:= GetString(NumStr, Length(NumStr) - 9);&#13;&#10;                  if (TmpStr = '1') then Res:= Res + Satu&#13;&#10;                  else Res:= Res + Terbilang(StrToFloat(TmpStr));&#13;&#10;                  Res:= Res + 'Milyar ';&#13;&#10;                end;&#13;&#10;              13..15 :&#13;&#10;                begin&#13;&#10;                  TmpStr:= GetString(NumStr, Length(NumStr) - 12);&#13;&#10;                  if (TmpStr = '1') then Res:= Res + Satu&#13;&#10;                  else Res:= Res + Terbilang(StrToFloat(TmpStr));&#13;&#10;                  Res:= Res + 'Trilyun ';&#13;&#10;                end;&#13;&#10;            end;&#13;&#10;            Res:= Res + Terbilang(StrToFloat(GetString(NumStr,&#13;&#10;                        Length(NumStr) - Length(TmpStr), 2)));&#13;&#10;          end&#13;&#10;        else if (Length(NumStr)&#62;15) then&#13;&#10;          begin&#13;&#10;            //Res:= Res + TextNumeric(StrToFloat(LeftStr(NumStr,&#13;&#10;            //            Length(NumStr) - 15)));&#13;&#10;            //Res:= Res + TextNumeric(StrToFloat(RightStr(NumStr, 15)));&#13;&#10;            Res:= '-';&#13;&#10;          end;&#13;&#10;      end;&#13;&#10;  end;&#13;&#10;  if ((Desimal&#62;0) and UseDecimal) then&#13;&#10;    begin&#13;&#10;      if (Angka = 0) then&#13;&#10;        Res:= Res + 'Nol '&#13;&#10;      else&#13;&#10;        Res:= Res + 'Koma ';&#13;&#10;      Res:= Res + Terbilang(Desimal);&#13;&#10;    end;&#13;&#10;  Result:= Res;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTanggalTTD: String;&#13;&#10;var Tgl: TDateTime;&#13;&#10;begin&#13;&#10;  Result:= '';&#13;&#10;  if (TglTTD='') then exit;&#13;&#10;  Tgl:= StrToDate(TglTTD);&#13;&#10;  Result:= FormatDateTime('dd', Tgl);&#13;&#10;  Result:= Result + ' ' + strBulan[MonthOf(Tgl)-1] + ' ' + IntToStr(YearOf(Tgl));&#13;&#10;end;&#13;&#10;&#13;&#10;function GetBulan(Bln: Integer): String;&#13;&#10;begin&#13;&#10;  Result:= strBulan[Bln-1];&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qryUnitTahun.Close;&#13;&#10;  qryUnitTahun.Params[0].Value:= Tahun;&#13;&#10;  qryUnitTahun.Params[1].value:= &#60;qryUnit.&#34;SKPD&#34;&#62;;&#13;&#10;  qryUnitTahun.Open;&#13;&#10;  qryAktif.Close;&#13;&#10;  qryAktif.Params[0].Value:= &#60;qryGaji.&#34;Jenis&#34;&#62;;&#13;&#10;  qryAktif.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  qryGaji.Close;&#13;&#10;  qryUnit.Close;&#13;&#10;  strWhere:= strWhere + ' and (tahun=''' + Tahun + ''') ';&#13;&#10;  strWhere:= strWhere + ' and (NIP=''' + NIP + ''') ';&#13;&#10;  ReplaceStr(qryGaji.SQL.Text, '--strWhere', strWhere);&#13;&#10;  qryGaji.Open;&#13;&#10;  qryUnit.Open;&#13;&#10;end." PropData="">
