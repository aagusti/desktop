    <TfrxADOQuery Name="qryUnitTahun" UserName="qryUnitTahun" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT A.*, B.BMNIP AS PengelolaNIP, B.BMNama AS PengelolaNama&#13;&#10;FROM UNITTAHUN A  LEFT OUTER JOIN UNITTAHUN B ON B.TAHUN=A.TAHUN AND B.UNITKD=LEFT(A.UNITKD, 7)&#13;&#10;WHERE&#13;&#10;A.TAHUN=:TAHUN&#13;&#10;AND&#13;&#10;A.UNITKD=:UNITKD" CommandTimeout="30" PropData="0A506172616D657465727301010C21000000204E616D653D22544148554E222044617461547970653D226674537472696E672200010C22000000204E616D653D22554E49544B44222044617461547970653D226674537472696E67220000"/>
    <TfrxADOQuery Name="qryRekap" UserName="qryRekap" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT G.GOLONGANKD, G.URAIAN, VG.TAHUN, VG.BULAN, VG.JENIS,&#13;&#10;ISNULL(COUNT(VG.NIP), 0) AS JML_PGW,&#13;&#10;ISNULL((SELECT COUNT(NIP) FROM VIEWGAJI WHERE&#13;&#10;TAHUN=VG.TAHUN AND BULAN=VG.BULAN AND JENIS=VG.JENIS&#13;&#10;AND GOLONGANKD=G.GOLONGANKD + '/A' AND 1=1), 0) AS JML_PGW_GA,&#13;&#10;ISNULL((SELECT COUNT(NIP) FROM VIEWGAJI WHERE&#13;&#10;TAHUN=VG.TAHUN AND BULAN=VG.BULAN AND JENIS=VG.JENIS&#13;&#10;AND GOLONGANKD=G.GOLONGANKD + '/B' AND 1=1), 0) AS JML_PGW_GB,&#13;&#10;ISNULL((SELECT COUNT(NIP) FROM VIEWGAJI WHERE&#13;&#10;TAHUN=VG.TAHUN AND BULAN=VG.BULAN AND JENIS=VG.JENIS&#13;&#10;AND GOLONGANKD=G.GOLONGANKD + '/C' AND 1=1), 0) AS JML_PGW_GC,&#13;&#10;ISNULL((SELECT COUNT(NIP) FROM VIEWGAJI WHERE&#13;&#10;TAHUN=VG.TAHUN AND BULAN=VG.BULAN AND JENIS=VG.JENIS&#13;&#10;AND GOLONGANKD=G.GOLONGANKD + '/D' AND 1=1), 0) AS JML_PGW_GD,&#13;&#10;ISNULL((SELECT COUNT(NIP) FROM VIEWGAJI WHERE&#13;&#10;TAHUN=VG.TAHUN AND BULAN=VG.BULAN AND JENIS=VG.JENIS&#13;&#10;AND GOLONGANKD=G.GOLONGANKD + '/E' AND 1=1), 0) AS JML_PGW_GE,&#13;&#10;ISNULL(SUM(VG.JML_SI), 0) AS JML_SI,&#13;&#10;ISNULL(SUM(VG.JML_ANAK), 0) AS JML_ANAK,&#13;&#10;ISNULL(SUM(VG.GAJI_POKOK), 0) AS GAJI_POKOK,&#13;&#10;ISNULL(SUM(VG.TUNJ_ISTRI), 0) AS TUNJ_ISTRI,&#13;&#10;ISNULL(SUM(VG.TUNJ_ANAK), 0) AS TUNJ_ANAK,&#13;&#10;ISNULL(SUM(VG.TUNJ_BERAS), 0) AS TUNJ_BERAS,&#13;&#10;ISNULL(SUM(VG.TUNJ_JAB_STRUKTUR), 0) AS TUNJ_JAB_STRUKTUR,&#13;&#10;ISNULL(SUM(VG.TUNJ_JAB_FUNGSI), 0) AS TUNJ_JAB_FUNGSI,&#13;&#10;ISNULL(SUM(VG.TUNJ_UMUM), 0) AS TUNJ_UMUM,&#13;&#10;ISNULL(SUM(VG.TUNJ_UMUM_TAMB), 0) AS TUNJ_UMUM_TAMB,&#13;&#10;ISNULL(SUM(VG.PPH), 0) AS PPH,&#13;&#10;ISNULL(SUM(VG.PEMBULATAN), 0) AS PEMBULATAN,&#13;&#10;ISNULL(SUM(VG.POT_IWP), 0) AS POT_IWP,&#13;&#10;ISNULL(SUM(VG.POT_TAPERUM), 0) AS POT_TAPERUM&#13;&#10;&#13;&#10;FROM&#13;&#10;(SELECT *&#13;&#10;FROM tblGolongan&#13;&#10;WHERE LEFT(GOLONGANKD, 2) &#60;&#62; 'VI' AND ISSUMMARY=1) G&#13;&#10;LEFT OUTER JOIN&#13;&#10;(SELECT * FROM VIEWGAJI WHERE TAHUN='&#60;TAHUN&#62;' AND BULAN='&#60;BULAN&#62;' AND JENIS=9999 AND 1=1) VG&#13;&#10;ON G.GOLONGANKD = LEFT(VG.GOLONGANKD, LEN(VG.GOLONGANKD)-2)&#13;&#10;&#13;&#10;GROUP BY G.GOLONGANKD, G.URAIAN, VG.TAHUN, VG.BULAN, VG.JENIS&#13;&#10;ORDER BY G.GOLONGANKD" CommandTimeout="500" PropData="0A506172616D65746572730100"/>
