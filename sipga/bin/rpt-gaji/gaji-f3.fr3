    <TfrxADOQuery Name="qryUnit" UserName="qryUnit" CloseDataSource="True" FieldAliases.Text="" Master="qryJenis" SQL.Text="select a.*, b.SKPDNM&#13;&#10;from&#13;&#10;(select *, SKPD=Left(UnitKd, 7) from tblUnit)a&#13;&#10;inner join&#13;&#10;(SELECT UnitKd as SKPD, UnitNm as SKPDNM From tblUnit Where Len(UnitKd)=7)b&#13;&#10;on a.SKPD=b.SKPD&#13;&#10;where a.UnitKd=:UnitKd" CommandTimeout="30" PropData="0A506172616D657465727301010C22000000204E616D653D22556E69744B64222044617461547970653D226674537472696E67220000"/>
