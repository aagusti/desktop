<TfrxReport Version="3.22" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="3577" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40848.0251090394" ReportOptions.Description.Text="" ReportOptions.LastChange="41281.8102865394" ScriptLanguage="PascalScript" ScriptText.Text="{&#13;&#10;daftar default parameter report :&#13;&#10;&#13;&#10;UnitKd&#13;&#10;Tahun&#13;&#10;Bulan&#13;&#10;GolKd&#13;&#10;GolonganKd&#13;&#10;UserId&#13;&#10;Jenis&#13;&#10;SubUnit&#13;&#10;Cari&#13;&#10;TglTTD&#13;&#10;}&#13;&#10;&#13;&#10;const strBulan = ['Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni', 'Juli', 'Agustus', 'September', 'Oktober', 'Nopember', 'Desember'];&#13;&#10;&#13;&#10;var strSQL, strFilter: String;&#13;&#10;       JnsGaji, DefaultQuery: String;&#13;&#10;       UseSubUnit: Boolean;&#13;&#10;       NoUrut: Integer;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTanggalTTD: String;&#13;&#10;var Tgl: TDateTime;&#13;&#10;begin&#13;&#10;  Result:= '';&#13;&#10;  if (TglTTD='') then exit;&#13;&#10;  Tgl:= StrToDate(TglTTD);&#13;&#10;  Result:= FormatDateTime('dd', Tgl);&#13;&#10;  Result:= Result + ' ' + strBulan[MonthOf(Tgl)-1] + ' ' + IntToStr(YearOf(Tgl));&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  case (IntToStr(Jenis)) of&#13;&#10;  1:   JnsGaji:= 'Gaji 13';&#13;&#10;  2:   JnsGaji:= 'Uang Duka Wafat';&#13;&#10;  3:   JnsGaji:= 'Uang Duka Tewas';&#13;&#10;  6:   JnsGaji:= 'Gaji Terusan';&#13;&#10;  else   JnsGaji:= 'Gaji Induk';&#13;&#10;  end;&#13;&#10;  qryJenis.Close;&#13;&#10;  qryUnit.Close;&#13;&#10;  qryUnitTahun.Close;&#13;&#10;  qryBulan.Close;&#13;&#10;  qryRekap.Close;&#13;&#10;  DefaultQuery:=  qryRekap.SQL.Text;&#13;&#10;  qryUser.Close;&#13;&#10;  qryUser.Params[0].Value:= UserId;&#13;&#10;  qryUser.Open;&#13;&#10;  UseSubUnit:= ((SubUnit='1') and (&#60;qryUser.&#34;ACCESSSUB_UNIT&#34;&#62;));&#13;&#10;  ReplaceStr(DefaultQuery, '&#60;TAHUN&#62;', Tahun);&#13;&#10;  ReplaceStr(DefaultQuery, '&#60;BULAN&#62;', Bulan);&#13;&#10;  ReplaceStr(DefaultQuery, '9999', Jenis);&#13;&#10;  strFilter:= '';&#13;&#10;  if (GolonganKd&#60;&#62;'') then strFilter:= ' AND (GolonganKd=''' + GolonganKd + ''') ';&#13;&#10;  if ((GolKd&#60;&#62;'') and (GolonganKd='')) then&#13;&#10;       strFilter:= ' AND (GolonganKd Like ''' + GolKd + '/%'') ';&#13;&#10;  if (UnitKd&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       if (UseSubUnit) then&#13;&#10;               strFilter:= strFilter + ' AND UNITKD Like ''' + UnitKd + '%''  '&#13;&#10;       else&#13;&#10;               strFilter:= strFilter + ' AND UNITKD=''' + UnitKd + '''  ';&#13;&#10;  end;&#13;&#10;  if (Cari&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       strFilter:= strFilter + ' and ((NIP Like ''%' + Cari + '%'') or (Nama Like ''%' + Cari + '%'')) ';&#13;&#10;  end;&#13;&#10;  ReplaceStr(DefaultQuery, 'AND 1=1', strFilter);&#13;&#10;  strFilter:= 'Jenis = ' + Jenis + ' ' +&#13;&#10;                       'And Tahun=''' + Tahun + ''' ' +&#13;&#10;                       'and Bulan=''' + Bulan + ''' ' + strFilter;&#13;&#10;  ReplaceStr(qryJenis.SQL.Text, '----', 'WHERE ' + strFilter);&#13;&#10;&#13;&#10;  qryJenis.Open;&#13;&#10;  qryUnit.Open;&#13;&#10;  qryBulan.Open;&#13;&#10;  qryUnitTahun.Params[0].Value:= Tahun;&#13;&#10;  qryUnitTahun.Params[1].Value:=  UnitKd;&#13;&#10;  qryUnitTahun.Open;&#13;&#10;  qryRekap.SQL.Text:= DefaultQuery;&#13;&#10;  qryRekap.Open;&#13;&#10;end.&#13;&#10;" PropData="">
