<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="3577" PreviewOptions.Zoom="1" PrintOptions.Printer="EPSON LQ-2180 ESC/P2" ReportOptions.CreateDate="40847.6260390741" ReportOptions.Description.Text="" ReportOptions.LastChange="41283.0337828125" ScriptLanguage="PascalScript" ScriptText.Text="{&#13;&#10;daftar default parameter report :&#13;&#10;&#13;&#10;UnitKd&#13;&#10;Tahun&#13;&#10;Bulan&#13;&#10;GolKd&#13;&#10;GolonganKd&#13;&#10;UserId&#13;&#10;Jenis&#13;&#10;SubUnit&#13;&#10;Cari&#13;&#10;TglTTD&#13;&#10;}&#13;&#10;&#13;&#10;Const KGB= 24;&#13;&#10;       strBulan = ['Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni', 'Juli', 'Agustus', 'September', 'Oktober', 'Nopember', 'Desember'];&#13;&#10;&#13;&#10;&#13;&#10;var    strSQL: String;&#13;&#10;       JnsGaji, strFilter: String;&#13;&#10;       UseSubUnit: Boolean;&#13;&#10;       NoUrut: Integer;&#13;&#10;       strSisaKGB: String;&#13;&#10;&#13;&#10;&#13;&#10;function GetTerusan(AktifTgl: TDateTime; Thn, Bln: String): String;&#13;&#10;var Bulan: String;&#13;&#10;    nTahun, AktifThn, AktifBln, nBln: Integer;&#13;&#10;begin&#13;&#10;       Result:= '0';&#13;&#10;       AktifThn:= YearOf(AktifTgl);&#13;&#10;       if (AktifThn&#60;=1900) then exit;&#13;&#10;       nTahun:= StrToInt(Thn) - AktifThn;&#13;&#10;       nTahun:= nTahun * 12;&#13;&#10;       if (nTahun&#60;0) then exit;&#13;&#10;       AktifBln:= MonthOf(AktifTgl);&#13;&#10;       nBln:= (nTahun - AktifBln) + StrToInt(Bln);&#13;&#10;       Result:= IntToStr(nBln);&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTanggalTTD: String;&#13;&#10;var Tgl: TDateTime;&#13;&#10;begin&#13;&#10;  Result:= '';&#13;&#10;  if (TglTTD='') then exit;&#13;&#10;  Tgl:= StrToDate(TglTTD);&#13;&#10;  Result:= FormatDateTime('dd', Tgl);&#13;&#10;  Result:= Result + ' ' + strBulan[MonthOf(Tgl)-1] + ' ' + IntToStr(YearOf(Tgl));&#13;&#10;end;&#13;&#10;&#13;&#10;function GetMasaKGB(Tgl: TDateTime): TDateTime;&#13;&#10;var D, M, Y: Integer;&#13;&#10;       S: String;&#13;&#10;begin&#13;&#10;  D:= DayOf(Tgl); M:= MonthOf(Tgl); Y:= YearOf(Tgl);&#13;&#10;  Y:= Y + 2;&#13;&#10;  S:= IntToStr(M) + '/' + IntToStr(D) + '/' + IntToStr(Y);&#13;&#10;  Result:= StrToDate(S);&#13;&#10;end;&#13;&#10;&#13;&#10;function MonthBetween(TglLast, TglNew: TDateTime): Integer;&#13;&#10;var N, D1, D2: integer;&#13;&#10;begin&#13;&#10;  D1:= DayOf(TglLast); D2:= DayOf(TglNew);&#13;&#10;  if (YearOf(TglLast)=YearOf(TglNew)) then&#13;&#10;       N:= MonthOf(TglNew) - MonthOf(TglLast)&#13;&#10;  else&#13;&#10;  begin&#13;&#10;&#13;&#10;       if (YearOf(TglLast)&#60;YearOf(TglNew)) then&#13;&#10;               N:= MonthOf(TglNew) + ((12*(YearOf(TglNew)-YearOf(TglLast)))-MonthOf(TglLast))&#13;&#10;       else&#13;&#10;               N:= MonthOf(TglLast) + ((12*(YearOf(TglLast)-YearOf(TglNew)))-MonthOf(TglNew));&#13;&#10;  end;&#13;&#10;  if (D1&#62;D2) then N:= N-1;&#13;&#10;  Result:= N;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetSisaKGB(TMT: TDateTime): String;&#13;&#10;var N: Integer;&#13;&#10;       R: String;&#13;&#10;begin&#13;&#10;  if (YearOf(TMT)&#60;=1910) then   // data = null&#13;&#10;       R:= '--'&#13;&#10;  else&#13;&#10;  begin&#13;&#10;       N:= MonthBetween(StrToDate(TglTTD), GetMasaKGB(TMT));&#13;&#10;       R:= FormatFloat('00', N);&#13;&#10;  end;&#13;&#10;  Result:= R;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Inc(NoUrut);&#13;&#10;  strSisaKGB:= GetSisaKGB(&#60;qryGaji.&#34;TMT_MasaKerja&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupFooter2OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  NoUrut:= 0;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  NoUrut:= 0;&#13;&#10;  case (IntToStr(Jenis)) of&#13;&#10;  1:   JnsGaji:= 'Gaji 13';&#13;&#10;  2:   JnsGaji:= 'Uang Duka Wafat';&#13;&#10;  3:   JnsGaji:= 'Uang Duka Tewas';&#13;&#10;  6:   JnsGaji:= 'Gaji Terusan';&#13;&#10;  else   JnsGaji:= 'Gaji Induk';&#13;&#10;  end;&#13;&#10;  qryGaji.Close;&#13;&#10;  qryUnit.Close;&#13;&#10;  qryBulan.Close;&#13;&#10;  qryUser.Close;&#13;&#10;  qryUser.Params[0].Value:= UserId;&#13;&#10;  qryUser.Open;&#13;&#10;&#13;&#10;  UseSubUnit:= ((SubUnit='1') and (&#60;qryUser.&#34;ACCESSSUB_UNIT&#34;&#62;));&#13;&#10;&#13;&#10;  strSQL:= 'SELECT *, GOL=LEFT(GOLONGANKD, LEN(GOLONGANKD)-2), ' +&#13;&#10;       'case when sts_pegawaikd=''P'' then ''PNS'' else ''CPNS'' end as sts_peg From ViewGaji ';&#13;&#10;&#13;&#10;  strFilter:= 'Jenis = ' + Jenis + ' ' +&#13;&#10;                       'And Tahun=''' + Tahun + ''' ' +&#13;&#10;                       'and Bulan=''' + Bulan + ''' ';&#13;&#10;  if (GolonganKd&#60;&#62;'') then strFilter:= strFilter + ' and (GolonganKd=''' + GolonganKd + ''') ';&#13;&#10;  if ((GolKd&#60;&#62;'') and (GolonganKd='')) then&#13;&#10;       strFilter:= strFilter + ' and (GolonganKd Like ''' + GolKd + '/%'') ';&#13;&#10;&#13;&#10;  if (UnitKd&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       if (UseSubUnit) then&#13;&#10;               strFilter:= strFilter + ' and (UnitKd Like ''' + UnitKd + '%'') '&#13;&#10;       else&#13;&#10;               strFilter:= strFilter + ' and (UnitKd=''' + UnitKd + ''') ';&#13;&#10;  end;&#13;&#10;  if (Cari&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       strFilter:= strFilter + ' and ((NIP Like ''%' + Cari + '%'') or (Nama Like ''%' + Cari + '%'')) ';&#13;&#10;  end;&#13;&#10;&#13;&#10;  strSQL:= strSQL + ' Where ' + strFilter;&#13;&#10;&#13;&#10;  strSQL:= strSQL + ' ORDER BY Unitkd ASC, sts_pegawaikd desc, ' +&#13;&#10;       '(case jbt_strukturkd when ''9999'' then ''zzz'' when ''BUP'' then ''0'' when ''WABUP'' then ''1'' ' +&#13;&#10;       'else jbt_strukturkd end)asc, golongankd desc, Masakerja desc,  ' +&#13;&#10;       'aktif_kd asc ';&#13;&#10;&#13;&#10;&#13;&#10;  qryGaji.SQL.Text:= strSQL;&#13;&#10;  qryGaji.Open;&#13;&#10;  qryUnit.Close;&#13;&#10;  qryBulan.Close;&#13;&#10;  qryUnit.Open;&#13;&#10;  qryBulan.Open;&#13;&#10;end." PropData="">
