<TfrxReport Version="3.22" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="3577" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40852.4363389931" ReportOptions.Description.Text="" ReportOptions.LastChange="40852.5228157986" ScriptLanguage="PascalScript" ScriptText.Text="{&#13;&#10;daftar default parameter report :&#13;&#10;&#13;&#10;UnitKd&#13;&#10;Tahun&#13;&#10;Bulan&#13;&#10;GolKd&#13;&#10;GolonganKd&#13;&#10;UserId&#13;&#10;Cari&#13;&#10;}&#13;&#10;&#13;&#10;const strBulan = ['', 'Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni', 'Juli', 'Agustus', 'September', 'Oktober', 'Nopember', 'Desember'];&#13;&#10;&#13;&#10;var BulanNm, strSQL: String;&#13;&#10;       UseSubUnit: Boolean;&#13;&#10;       NoUrut: Integer;&#13;&#10;&#13;&#10;&#13;&#10;procedure GroupFooter1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  NoUrut:= 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Inc(NoUrut);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qryUnitTahun.Close;&#13;&#10;  qryUnitTahun.Params[0].Value:= Tahun;&#13;&#10;  qryUnitTahun.Params[1].Value:=  &#60;qryMonPensiun.&#34;SKPD&#34;&#62;;&#13;&#10;  qryUnitTahun.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  NoUrut:= 0;&#13;&#10;  qryUser.Close;&#13;&#10;  qryUser.Params[0].Value:= UserId;&#13;&#10;  qryUser.Open;&#13;&#10;  UseSubUnit:= &#60;qryUser.&#34;ACCESSSUB_UNIT&#34;&#62;;&#13;&#10;&#13;&#10;  strSQL:= 'select a.*, u.UnitNm as SKPDNm ' +&#13;&#10;               'from ' +&#13;&#10;               '(select *, SKPD = Left(UnitKd, 7) from ViewPegawai)a ' +&#13;&#10;               'inner join (select *, SKPD=UnitKd from tblUnit)u on a.SKPD=u.SKPD ' +&#13;&#10;               'Where ' +&#13;&#10;               '(not a.Aktif_Kd in(3,4,5,6)) and (not a.Tgl_Lahir Is Null) ' +&#13;&#10;               'and (not a.Tgl_BUP Is Null) AND (YEAR(a.TGL_BUP) = ' + Tahun + ') ';&#13;&#10;&#13;&#10;  if (StrToInt(Bulan)&#62;0) then&#13;&#10;  begin&#13;&#10;       BulanNm:= UpperCase(strBulan[StrToInt(Bulan)]);&#13;&#10;       strSQL:= strSQL + 'and (Month(a.TGL_BUP)=' + Bulan + ') ';&#13;&#10;  end&#13;&#10;  else BulanNm:= UpperCase(strBulan[1] + ' s/d ' + strBulan[12]);&#13;&#10;&#13;&#10;  if (GolonganKd&#60;&#62;'') then strSQL:= strSQL + ' and (a.GolonganKd=''' + GolonganKd + ''') ';&#13;&#10;  if ((GolKd&#60;&#62;'') and (GolonganKd='')) then&#13;&#10;       strSQL:= strSQL + ' and (a.GolonganKd Like ''' + GolKd + '/%'') ';&#13;&#10;&#13;&#10;  if (UnitKd&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       if (UseSubUnit) then&#13;&#10;               strSQL:= strSQL + ' and (a.UnitKd Like ''' + UnitKd + '%'') '&#13;&#10;       else&#13;&#10;               strSQL:= strSQL + ' and (a.UnitKd=''' + UnitKd + ''') ';&#13;&#10;  end;&#13;&#10;  if (Cari&#60;&#62;'') then&#13;&#10;       strSQL:= strSQL + ' and ((a.NIP Like ''' + Cari + '%'') and (a.Nama Like ''%' + Cari + '%'')) ';&#13;&#10;&#13;&#10;  strSQL:= strSQL + ' order by a.Unitkd, a.sts_pegawaikd desc, a.aktif_kd asc, ' +&#13;&#10;      '(case a.jbt_strukturkd when ''9999'' then ''zzz'' when ''BUP'' then ''0'' ' +&#13;&#10;      'when ''WABUP'' then ''1'' else a.jbt_strukturkd end)asc, a.golongankd desc';&#13;&#10;  //ShowMessage(strSQL);&#13;&#10;  qryMonPensiun.Close;&#13;&#10;  qryMonPensiun.SQL.Text:= strSQL;&#13;&#10;  qryMonPensiun.Open;&#13;&#10;end." PropData="">
