SKPDNm ' +&#13;&#10;               'from ' +&#13;&#10;               '(select *, SKPD = Left(UnitKd, 7) from ViewTertanggung)a ' +&#13;&#10;               'inner join (select *, SKPD=UnitKd from tblUnit)u on a.SKPD=u.SKPD ' +&#13;&#10;               'Where ' +&#13;&#10;               '(not a.Aktif_Kd in(3,4,5,6)) and ((a.Hub_Kd&#60;&#62;''SU'') and (a.Hub_Kd&#60;&#62;''IS'')) and (a.IsSekolah=0) ' +&#13;&#10;               'and (not a.TanggalLahir Is Null) and (YEAR(a.TanggalLahir) + 21 &#60; ' + Tahun + ') ';&#13;&#10;&#13;&#10;  if (StrToInt(Bulan)&#62;0) then&#13;&#10;  begin&#13;&#10;       BulanNm:= UpperCase(strBulan[StrToInt(Bulan)]);&#13;&#10;       strSQL:= strSQL + 'and (Month(a.TanggalLahir)=' + Bulan + ') ';&#13;&#10;  end&#13;&#10;  else BulanNm:= UpperCase(strBulan[1] + ' s/d ' + strBulan[12]);&#13;&#10;&#13;&#10;  if (GolonganKd&#60;&#62;'') then strSQL:= strSQL + ' and (a.GolonganKd=''' + GolonganKd + ''') ';&#13;&#10;  if ((GolKd&#60;&#62;'') and (GolonganKd='')) then&#13;&#10;       strSQL:= strSQL + ' and (a.GolonganKd Like ''' + GolKd + '/%'') ';&#13;&#10;&#13;&#10;  if (UnitKd&#60;&#62;'') then&#13;&#10;  begin&#13;&#10;       if (UseSubUnit) then&#13;&#10;               strSQL:= strSQL + ' and (a.UnitKd Like ''' + UnitKd + '%'') '&#13;&#10;       else&#13;&#10;               strSQL:= strSQL + ' and (a.UnitKd=''' + UnitKd + ''') ';&#13;&#10;  end;&#13;&#10;  if (Cari&#60;&#62;'') then&#13;&#10;       strSQL:= strSQL + ' and ((a.NIP Like ''' + Cari + '%'') and (a.Nama Like ''%' + Cari + '%'')) ';&#13;&#10;&#13;&#10;  strSQL:= strSQL + ' order by a.Unitkd, a.TMT_GAJI_POKOK, a.sts_pegawaikd desc, a.aktif_kd asc, ' +&#13;&#10;      '(case a.jbt_strukturkd when ''9999'' then ''zzz'' when ''BUP'' then ''0'' ' +&#13;&#10;      'when ''WABUP'' then ''1'' else a.jbt_strukturkd end)asc, a.golongankd desc';&#13;&#10;  //ShowMessage(strSQL);&#13;&#10;  qryMonAnak.Close;&#13;&#10;  qryMonAnak.SQL.Text:= strSQL;&#13;&#10;  qryMonAnak.Open;&#13;&#10;end." Prop